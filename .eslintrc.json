{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "no-return-assign": "off",
        "global-require": "off"
    }
}
