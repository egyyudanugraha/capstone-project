{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "max-len": "off",
        "no-return-assign": "off",
        "no-undef": "off",
        "no-nested-ternary":"off"
    }
}
